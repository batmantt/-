
.integral-list-container {
  background: #f4f4f4;
  height: 100vh;
  width: 100vw;
}

.integral-list-container .integral-item {
  height: 105rpx;
  width: 702rpx;
  background: #fff;
  border-radius:5rpx;
  display: flex;
  margin-left: 24rpx;
  margin-top: 28rpx;
}

.time {
  width: 135rpx;
  height: 105rpx;
  background: #fbf3be;
  font-size: 22rpx;
  display: flex;
  align-items: center;
  border-radius:5rpx;
}

.date {
  font-size: 22rpx;
  margin: 0 auto;
  text-align: center;
  color: #000;
}
.gmvTime{
  margin-bottom: 9rpx;
}
.right_info {
  font-size: 26rpx;
  letter-spacing: 2rpx;
  padding: 15rpx 28rpx;
  width: 582rpx;
  color: #000;
}

.gt, .et{
   display: flex; 
   justify-content: space-between;  
}