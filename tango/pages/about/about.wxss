/* pages/about/about.wxss */
.about-container{
  min-height: 100vh;
  box-sizing:border-box;
  font-size:26rpx;
  padding:0 48rpx;
  font-family: "PingFang-SC-Medium";
}
.about-container .logo-wrapper{
  width:194rpx;
  height:194rpx;
  margin:150rpx auto 100rpx auto;
  border-radius: 50%;
  overflow: hidden;
}
.about-container .logo-wrapper .img{
  width:100%;
  height:100%;
}
.about-container .desc{
  color:#999;
  line-height: 39rpx;
  margin-bottom:32rpx;
}